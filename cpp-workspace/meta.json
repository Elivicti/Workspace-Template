{
	"$schema": "../schemas/template.json",
	"title": "C++ Workspace Templates",
	"description": "",
	"variants": {
		"base": { "path": "base" },
		"default": {
			"alias": [ "generic", "cmdline" ],
			"path": "default",
			"inherits": "base"
		},
		"qt": {
			"alias": [ "gui-qt" ],
			"path": "qt",
			"inherits": "default"
		}
	},
	"project_file_patterns": [ "CMakeLists.txt" ],
	"default": "default"
}
